<p id="notice"><%= notice %></p>

<h1>Inquiries</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Dob</th>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Insurance carrier</th>
      <th>Member</th>
      <th>Policy holder</th>
      <th>Policy holder name</th>
      <th>Policy holder dob</th>
      <th>Insurance phone number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inquiries.each do |inquiry| %>
      <tr>
        <td><%= inquiry.first_name %></td>
        <td><%= inquiry.last_name %></td>
        <td><%= inquiry.DOB %></td>
        <td><%= inquiry.street %></td>
        <td><%= inquiry.city %></td>
        <td><%= inquiry.state %></td>
        <td><%= inquiry.zip %></td>
        <td><%= inquiry.insurance_carrier %></td>
        <td><%= inquiry.member_id %></td>
        <td><%= inquiry.policy_holder %></td>
        <td><%= inquiry.policy_holder_name %></td>
        <td><%= inquiry.policy_holder_dob %></td>
        <td><%= inquiry.insurance_phone_number %></td>
        <td><%= link_to 'Show', inquiry %></td>
        <td><%= link_to 'Edit', edit_inquiry_path(inquiry) %></td>
        <td><%= link_to 'Destroy', inquiry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inquiry', new_inquiry_path %>
